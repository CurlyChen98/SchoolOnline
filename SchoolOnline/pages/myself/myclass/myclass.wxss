.showman {
  margin-top: 10px;
}

.showman>.tip {
  padding-left: 10px;
  color: #9e9e9e;
  font-size: 13px;
  border-bottom: #f0f3fa 1px inset;
  line-height: 20px;
}

.showman>.tip>view>i {
  margin-right: 10px;
}

.showman>.man {
  border-bottom: #f0f3fa 1px inset;
  height: 50px;
  padding-left: 10px;
  align-items: center;
  display: flex;
  position: relative;
}

.showman>.man>i {
  margin-right: 5px;
  font-size: 20px;
  color: #3a3a3a;
}

.lock {
  opacity: 0;
}

.showman>.man>.man_button {
  display: flex;
  position: absolute;
  right: 5px;
}

.showman>.man>.man_button>.button-delete {
  text-align: center;
  background-color: #d9534f;
  color: #fff;
  width: 60px;
  line-height: 30px;
  border-radius: 5px;
  font-size: 13px;
  margin-right: 5px;
}
